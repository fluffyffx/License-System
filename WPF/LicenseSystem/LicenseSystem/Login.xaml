<Window x:Class="LicenseSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LicenseSystem"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Background="#202020"
        Title="MainWindow" Height="341" Width="640">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="159*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Your License Key" Foreground="White" Margin="1,31,-5,-31" Grid.Column="1"></Label>
        <TextBox x:Name="licenseTextbox" Margin="6,54,248,222" Grid.Column="1"></TextBox>
        <Button x:Name="login" Content="Login" Grid.Column="1" Margin="6,125,481,154" Click="login_Click"/>
    </Grid>
</Window>
